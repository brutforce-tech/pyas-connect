{
  "name": "@pyas/connect-react",
  "version": "1.0.11",
  "description": "A React wrapper around the Pyas Connect Web Component, exposing PyasConnect as a first-class React component",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/brutforce-tech/pyas-connect.git"
  },
  "homepage": "https://github.com/brutforce-tech/pyas-connect#readme",
  "author": "Pyas Team <support@pyas.io>",
  "license": "ISC",
  "keywords": [
    "pyas",
    "pyas-connect",
    "react",
    "reactjs",
    "oauth",
    "web-components",
    "calendar-integration",
    "google-calendar",
    "microsoft-graph"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./connect-react.css": "./dist/connect-react.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "dependencies": {
    "@pyas/connect": "^1.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
