{
  "name": "@pyas/connect-react",
  "version": "1.0.0",
  "description": "A React plugin wrapper around the Pyas Connect Web Component, exposing PyasConnect as a first-class React component",
  "keywords": [
    "pyas",
    "pyas-connect",
    "react",
    "react.js",
    "oauth",
    "web-components",
    "stencil",
    "plugin",
    "calendar-integration",
    "outlook api integration",
    "google calendar integration",
    "microsoft graph api",
    "google api"
  ],
  "author": "Pyas Team <support@pyas.io>",
  "homepage": "https://github.com/brutforce-tech/pyas-connect",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node ./__tests__/connect-react.test.js",
    "build": "npm run tsc",
    "tsc": "tsc -p . --outDir ./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stencil/react-output-target": "^1.0.2",
    "@pyas/connect": "*"
  }
}
