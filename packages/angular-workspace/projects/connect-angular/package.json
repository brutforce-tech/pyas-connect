{
  "name": "@pyas/connect-angular",
  "version": "1.0.9",
  "description": "An Angular plugin wrapper around the Pyas Connect Web Component, exposing PyasConnect as a first-class Angular component",
  "keywords": [
    "pyas",
    "pyas-connect",
    "angular",
    "angular19",
    "oauth",
    "web-components",
    "stencil",
    "plugin",
    "calendar-integration",
    "outlook api integration",
    "google calendar integration",
    "microsoft graph api",
    "google api"
  ],
  "scripts": {
    "build": "ng build connect-angular --configuration production"
  },
  "author": "Pyas Team <support@pyas.io>",
  "homepage": "https://github.com/brutforce-tech/pyas-connect",
  "main": "dist/fesm2022/index.mjs",
  "module": "dist/fesm2022/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/fesm2022/index.mjs",
      "require": "./dist/fesm2022/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "peerDependencies": {
    "@angular/common": ">=19.0.0",
    "@angular/core": ">=19.0.0",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "@pyas/connect": "^1.0.9"
  },
  "devDependencies": {
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "tslib": "^2.3.0",
    "typescript": "^5.4.5"
  },
  "sideEffects": false
}
