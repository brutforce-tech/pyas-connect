{
  "name": "@pyas/connect",
  "version": "1.0.8",
  "author": "Pyas Team <support@pyas.io>",
  "homepage": "https://github.com/brutforce-tech/pyas-connect",
  "description": "Pyas Connect is a framework-agnostic Web Component, built with Stencil, that embeds a full OAuth connect experience for Pyas customers in a single tag. The component talks directly to Pyas's auth endpoints, presents a branded modal for Google, Microsoft, Zoom, and other providers, manage token storage and completes the callback handshake â€” all so you can drop secure calendar-account linking into any web app without writing integration code.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pyas-connect/pyas-connect.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*",
      "require": "./dist/*"
    },
    "./hydrate": {
      "import": "./hydrate/index.js",
      "require": "./hydrate/index.mjs",
      "types": "./hydrate/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brutforce-tech/pyas-connect.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.10.2",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.0.2",
    "@stencil/vue-output-target": "^0.10.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.5.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
